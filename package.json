{
  "name": "vue-hackernews-2.0",
  "description": "A Vue.js project",
  "author": "Evan You <yyx990803@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "node server",
    "start": "cross-env NODE_ENV=production node server",
    "build": "rimraf dist && npm run build:client && npm run build:server && npm run build:api",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "build:api": "cross-env NODE_ENV=production webpack --config build/webpack.api.config.js --progress --hide-modules",
    "start:uat": "cross-env NODE_ENV=uat node server",
    "build:uat": "rimraf dist && npm run build:client:uat && npm run build:server:uat && npm run build:api:uat",
    "build:client:uat": "cross-env NODE_ENV=uat webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server:uat": "cross-env NODE_ENV=uat webpack --config build/webpack.server.config.js --progress --hide-modules",
    "build:api:uat": "cross-env NODE_ENV=uat webpack --config build/webpack.api.config.js --progress --hide-modules",
    "test": "cross-env NODE_ENV=dev jest --forceExit",
    "e2e": "rimraf tests/e2e/reports && cross-env NODE_ENV=testing node tests/e2e/runner.js"
  },
  "engines": {
    "node": ">=7.0",
    "npm": ">=4.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "verbose": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testMatch": [
      "**/src/**/__tests__/**/*.(js)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
