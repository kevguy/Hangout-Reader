{
  "name": "hangout-reader",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.com/kevguy/HeavenSent",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test",
    "e2e": "vue-cli-service e2e",
    "lint": "vue-cli-service lint",
    "deploy": "node ./node_modules/vue-gh-pages/index.js"
  },
  "dependencies": {
    "register-service-worker": "^1.0.0",
    "rxjs": "^5.5.6",
    "twemoji": "^2.5.0",
    "vue": "^2.5.13",
    "vue-gh-pages": "^1.8.8",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-alpha.4",
    "@vue/cli-plugin-e2e-nightwatch": "^3.0.0-alpha.4",
    "@vue/cli-plugin-eslint": "^3.0.0-alpha.4",
    "@vue/cli-plugin-pwa": "^3.0.0-alpha.4",
    "@vue/cli-plugin-unit-jest": "^3.0.0-alpha.4",
    "@vue/cli-service": "^3.0.0-alpha.4",
    "@vue/eslint-config-airbnb": "^3.0.0-alpha.4",
    "@vue/test-utils": "^1.0.0-beta.10",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.0.4",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "vue-template-compiler": "^2.5.13",
    "worker-loader": "^1.1.0"
  },
  "vue": {
    "lintOnSave": true
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "mapCoverage": true
  }
}
